{
  "name": "oscope",
  "version": "0.2.0",
  "description": "Simple command line tool for testing and interacting with OSC (Open Sound Control) messages",
  "bin": "./dist/cli/index.js",
  "main": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "scripts": {
    "start": "node ./dist/cli/index.js",
    "build": "parcel build",
    "dev": "tsc -w",
    "prepublishOnly": "rm -rf dist && npm run build"
  },
  "files": [
    "dist/"
  ],
  "author": "Matthew Kaney",
  "license": "0BSD",
  "repository": "github:mindofmatthew/oscope",
  "bugs": {
    "url": "https://github.com/mindofmatthew/oscope/issues"
  },
  "targets": {
    "bin": {
      "source": "src/cli/index.ts",
      "context": "node",
      "includeNodeModules": true,
      "sourceMap": false
    },
    "main": {
      "source": "src/osc/osc.ts",
      "context": "node"
    },
    "types": {
      "source": "src/osc/osc.ts"
    }
  },
  "devDependencies": {
    "@parcel/transformer-typescript-types": "^2.0.0-rc.0",
    "@types/node": "^14.14.30",
    "chalk": "^4.1.0",
    "moo": "^0.5.1",
    "parcel": "^2.0.0-rc.0",
    "typescript": "^4.1.5"
  }
}
